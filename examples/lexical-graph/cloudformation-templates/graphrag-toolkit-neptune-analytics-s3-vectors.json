                    {
                      "partition": {
                        "Ref": "AWS::Partition"
                      },
                      "region": {
                        "Ref": "AWS::Region"
                      },
                      "accountId": {
                        "Ref": "AWS::AccountId"
                      },
                      "regionPrefix": {
                        "Fn::Select": [
                          0,
                          {
                            "Fn::Split": [
                              "-",
                              {
                                "Ref": "AWS::Region"
                              }
                            ]
                          }
                        ]
                      }
                    }
                  ]
                },
