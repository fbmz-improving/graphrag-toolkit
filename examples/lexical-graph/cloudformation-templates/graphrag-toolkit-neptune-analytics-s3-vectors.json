    "DeleteVectorBucketFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "FunctionName": {
          "Fn::Sub": [
            "${prefix}-delete-vector-bucket",
            {
              "prefix": {
                "Ref": "ApplicationId"
              }
            }
          ]
        },
        "Handler": "index.lambda_handler",
        "Role": {
          "Fn::GetAtt": [
            "LambdaFunctionRole",
            "Arn"
          ]
        },
        "Code": {
          "ZipFile": {
            "Fn::Join": [
              "",
              [
                "import boto3\n",
                "import cfnresponse\n",
                "\n",
                "s3_vectors_client = boto3.client('s3vectors', region_name='",
                {
                  "Ref": "AWS::Region"
                },
                "')\n",
                "\n",
                "def delete_vector_bucket(s3_vectors_client, bucket_name):\n",
                "    \n",
                "    list_indexes_response = s3_vectors_client.list_indexes(\n",
                "        vectorBucketName=bucket_name,\n",
                "        maxResults=100\n",
                "    )\n",
                "    \n",
                "    while list_indexes_response['indexes']:\n",
                "        \n",
                "        for index in list_indexes_response['indexes']:\n",
                "            \n",
                "            index_name = index['indexName']\n",
                "            \n",
                "            print(f'Deleting index {index_name}')\n",
                "            \n",
                "            s3_vectors_client.delete_index(\n",
                "                vectorBucketName=bucket_name,\n",
                "                indexName=index_name\n",
                "            )\n",
                "        \n",
                "        list_indexes_response = s3_vectors_client.list_indexes(\n",
                "            vectorBucketName=bucket_name,\n",
                "            maxResults=100\n",
                "        )\n",
                "        \n",
                "    print(f'Deleting bucket {bucket_name}')\n",
                "    \n",
                "    s3_vectors_client.delete_vector_bucket(\n",
                "        vectorBucketName=bucket_name\n",
                "    )\n",
                "    \n",
                "    \n",
                "def lambda_handler(event, context):\n",
                "    \n",
                "    print('Event: {}'.format(event))\n",
                "    print('Context: {}'.format(context))\n",
                "    \n",
                "    if event['RequestType'] == 'Create' or event['RequestType'] == 'Update':\n",
                "        \n",
                "        event_type = event['RequestType']\n",
                "        print(f'Ignoring {event_type} request')\n",
                "        cfnresponse.send(event, context, cfnresponse.SUCCESS, {'Message': 'OK'}, context.log_stream_name)\n",
                "                   \n",
                "    elif event['RequestType'] == 'Delete':\n",
                "        \n",
                "        print('Deleting vector indexes and bucket')\n",
                "        \n",
                "        bucket_name = event['ResourceProperties']['BucketName']\n",
                "        \n",
                "        try:        \n",
                "            delete_vector_bucket(s3_vectors_client, bucket_name)\n",
                "            \n",
                "            cfnresponse.send(event, context, cfnresponse.SUCCESS, {'BucketName': bucket_name}, context.log_stream_name)\n",
                "        except Exception as e:\n",
                "            cfnresponse.send(event, context, cfnresponse.FAILED, { 'Message': str(e) }, context.log_stream_name)\n",
                "        \n",
                "    else:\n",
                "        cfnresponse.send(event, context, cfnresponse.FAILED, { 'Message': 'Expected a Create, Delete or Update request' }, context.log_stream_name)\n"
              ]
            ]
          }
        },
        "MemorySize": 128,
        "Runtime": "python3.12",
        "Timeout": "900",
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Sub": "${ApplicationId}-delete-vector-bucket"
            }
          },
          {
            "Key": "StackId",
            "Value": {
              "Fn::Sub": "${AWS::StackName}"
            }
          },
          {
            "Key": "Stack",
            "Value": {
              "Fn::Sub": "${AWS::Region}-${AWS::StackId}"
            }
          },
          {
            "Key": "Application",
            "Value": {
              "Fn::Sub": "graphrag-toolkit:application-id:${ApplicationId}"
            }
          }
        ]
      }
    },
    "DeleteVectorBucketCustomResource": {
      "Type": "Custom::CustomResource",
      "Properties": {
        "ServiceToken": {
          "Fn::GetAtt": [
            "DeleteVectorBucketFunction",
            "Arn"
          ]
        },
        "BucketName": {
          "Fn::Sub": "${ApplicationId}-${AWS::Region}"
        }
      }
    },
    "NeptuneNotebookInstance": {
      "Type": "AWS::SageMaker::NotebookInstance",
      "Properties": {
        "InstanceType": {
          "Ref": "NotebookInstanceType"
        },
        "PlatformIdentifier": "notebook-al2-v3",
        "NotebookInstanceName": {
          "Fn::Sub": "aws-neptune-${ApplicationId}"
        },
        "RoleArn": {
          "Fn::GetAtt": [
            "GraphRAGClientRole",
            "Arn"
          ]
        },
        "LifecycleConfigName": {
          "Fn::GetAtt": [
            "NeptuneNotebookInstanceLifecycleConfig",
            "NotebookInstanceLifecycleConfigName"
          ]
        },
        "Tags": [
          {
            "Key": "StackId",
            "Value": {
              "Fn::Sub": "${AWS::StackId}"
            }
          },
          {
            "Key": "Stack",
            "Value": {
              "Fn::Sub": "${AWS::Region}-${AWS::StackName}"
            }
          },
          {
            "Key": "Application",
            "Value": {
              "Fn::Sub": "graphrag-toolkit:application-id:${ApplicationId}"
            }
          }
